@model Inventario.Models.Cliente

<h1>Crear Cliente</h1>

<form asp-action="Crear" method="post">
    <div class="form-group">
        <label for="NombreCompleto">Nombre Completo</label>
        <input type="text" class="form-control" asp-for="NombreCompleto" />
    </div>
    <div class="form-group">
        <label for="Email">Email</label>
        <input type="email" class="form-control" asp-for="Email" />
    </div>
    <div class="form-group">
        <label for="Telefono">Teléfono</label>
        <input type="text" class="form-control" asp-for="Telefono" />
    </div>
    

    <button type="submit" class="btn btn-success">Crear</button>
    <img width="200px" height="210px" src="https://th.bing.com/th/id/R.a1564c72644c57ac25c36d3c32a156c6?rik=3xOhQCD0x%2bKqAA&riu=http%3a%2f%2fwww.bobarron.com%2fwp-content%2fuploads%2f2016%2f05%2f10065298_l.jpg&ehk=gfhV3pEULXP8xwNQ8EeYJCHia5znn0lD%2b%2bUxjLvAjcQ%3d&risl=&pid=ImgRaw&r=0g" alt="Imagen" />
</form>